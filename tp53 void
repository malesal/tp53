#include<bits/stdc++.h>
using namespace std;

void Imprimir(int num);

int main() {
    int numero = 500;
    Imprimir(numero);

    return 0;
}
void Imprimir(int num) {
    for (int i = 1; i <= num; i++) {
        cout << i;
        if (i % 4 == 0)
            cout << " (Múltiplo de 4)";
        if (i % 9 == 0)
            cout << " (Múltiplo de 9)";
        cout << endl;

        if (i % 5 == 0)
            cout << "--------------------------" << endl;
    }
}
